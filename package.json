  "license": "BSD-3-Clause",
  "bugs": {
    "url": "https://github.com/IWSLLC/react-blog-examples/issues"
  },
  "homepage": "https://github.com/IWSLLC/react-blog-examples",
  "devDependencies": {
    "@babel/cli": "^7.12.8",
    "@babel/core": "^7.12.9",
    "@babel/plugin-proposal-object-rest-spread": "^7.12.1",
    "@babel/plugin-transform-runtime": "^7.12.1",
    "@babel/polyfill": "^7.12.1",
    "@babel/preset-env": "^7.12.7",
    "@babel/preset-react": "^7.12.7",
    "@babel/register": "^7.12.1",
    "@babel/runtime": "^7.12.5",
    "babel-eslint": "^10.1.0",
    "babel-loader": "^8.2.2",
    "chai": "^4.2.0",
    "core-js": "^3.8.0",
    "eslint": "^7.14.0",
    "eslint-plugin-import": "^2.22.1",
    "eslint-plugin-react": "^7.21.5",
    "jsdom": "^16.3.0",
    "mocha": "^8.2.1",
    "prop-types": "^15.7.2",
    "react": "^17.0.1",
